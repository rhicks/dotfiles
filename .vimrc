set t_Co=256
set encoding=utf-8
" Use the same symbols as TextMate for tabstops and EOLs
set list
set listchars=tab:▸\ ,eol:¬
set number
syntax enable
set showcmd
set showmode

set background=dark

" solarized options 
"let g:solarized_termcolors = 256
"let g:solarized_visibility = "high"
"let g:solarized_contrast = "high"
colorscheme solarized


"colorscheme molokai
